#include <iostream>
using namespace std; 
   
 void reversearry(int arry[],int siz){

int start=0,end=siz-1;
while(start<=end){
    int change = arry[start];
    arry[start]=arry[end];
    arry[end]=change;
    start++;
    end--;

}
}
int main(){
    int siz=7;
    int arry[7]{6,8,9,4,47,89,45};
     reversearry( arry,siz);
    for(int i=0;i<siz;i++){
        cout<<arry[i]<<endl;
    }


}


    
